{
  "name": "@tssc-plugins/backstage-community-plugin-argocd-backend",
  "version": "1.0.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-appstudio/tssc-backstage-plugins",
    "directory": "plugins/argocd-backend"
  },
  "backstage": {
    "role": "backend-plugin",
    "supported-versions": "1.42.5",
    "pluginId": "quay",
    "pluginPackages": [
      "@tssc-plugins/backstage-community-plugin-argocd-backend"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "scripts": {
    "tsc": "tsc",
    "build": "backstage-cli package build",
    "lint:check": "backstage-cli package lint",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "clean": "backstage-cli package clean",
    "clean-build": "rimraf dist dist-dynamic dist-scalprum dist-plugin",
    "export-dynamic:backend": "rhdh-cli plugin export --no-install --embed-package @backstage-community/plugin-argocd-backend",
    "export-dynamic:backend:postinstall": "cd dist-dynamic && yarn install --inline-builds",
    "package-dynamic": "rhdh-cli plugin package --export-to dist-plugin"
  },
  "dependencies": {
    "@backstage-community/plugin-argocd-backend": "1.0.2"
  },
  "devDependencies": {
    "@backstage/cli": "^0.34.5",
    "@red-hat-developer-hub/cli": "1.9.1",
    "typescript": "5.9.3"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ],
  "homepage": "https://red.ht/trusted",
  "bugs": "https://issues.redhat.com/browse/RHTAP",
  "author": "Red Hat",
  "keywords": [
    "support:production",
    "lifecycle:active"
  ]
}
